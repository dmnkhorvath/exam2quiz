        "defaults": {
          "unit": "opm",
          "custom": { "drawStyle": "line", "fillOpacity": 15 }
        },
        "overrides": []
      },
      "targets": [
        {
          "expr": "sum(rate(worker_jobs_completed_total[5m])) by (stage) * 60",
          "legendFormat": "{{ stage }}",
          "refId": "A"
        }
      ]
    },
    {
